{
  "name": "client-vite",
  "version": "1.0.0",
  "scripts": {
    "client": "vite",
    "server": "node -e \"import('path').then(p=>{import('child_process').then(c=>{const s=p.resolve(process.cwd(),'../server');c.execSync(`npm run dev --prefix \\\"${s}\\\"`,{stdio:'inherit'})})})\"",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "axios": "^1.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.2"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.0.0",
    "concurrently": "^8.2.0",
    "vite": "^4.5.0"
  }
}
